{
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.typo3.org/"
        }
    ],
    "name": "bk2k/bootstrap-package-box",
    "description": "",
    "type": "project",
    "license": "MIT",
    "homepage": "https://github.com/benjaminkott/bootstrap_package",
    "authors": [
        {
            "name": "Benjamin Kott",
            "email": "info@bk2k.info",
            "homepage": "http://www.bk2k.info",
            "role": "Developer"
        }
    ],
    "require": {        
        "typo3/cms": "dev-master",
        "bk2k/bootstrap-package": "dev-master"
    },
    "config": {
        "secure-http": false,
        "sort-packages": true,
        "process-timeout": 2000,
        "preferred-install": {
            "typo3/cms": "source",
            "bk2k/bootstrap-package": "source",
            "*": "dist"
        }
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "web"
        }
    }
}
